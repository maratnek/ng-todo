{
    "name": "Great Extension",
    "version": "1.0",
    "description": "Build an Extension with Angular",
    "manifest_version": 2,
    "permissions": [
      // "activeTab",
      // "webNavigation"
      "storage",
      "contextMenus",
      "notifications"
  ],

  "browser_action": {
    "default_icon": "logo/logo28.png",
    "default_popup": "index.html"
},
  "background": {
      "scripts": [
          "background.js",
          "runtime.js"
      ]
  }
